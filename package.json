{
    "name": "cthunline-web",
    "version": "1.1.0",
    "scripts": {
        "dev": "vite serve src --config vite.config.ts",
        "build": "vite build src --config vite.config.ts --outDir ../build",
        "prepare": "husky install",
        "lint": "npx eslint . --ext .js,.ts,.tsx --max-warnings=0 && npx prettier --check . && npx tsc --noEmit && npx stylelint \"**/*.css\" --max-warnings=0",
        "format": "npx eslint . --fix --ext .js,.ts,.tsx --max-warnings=0 && npx prettier --write . && npx tsc --noEmit && npx stylelint --fix \"**/*.css\" --max-warnings=0"
    },
    "dependencies": {
        "@cthunline/games": "^2.3.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.0",
        "axios": "^1.4.0",
        "dayjs": "^1.11.9",
        "eslint-plugin-risxss": "^2.1.0",
        "formik": "^2.4.2",
        "nanoid": "^3.3.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.5",
        "react-icons": "^4.10.1",
        "react-router-dom": "^6.14.1",
        "react-toastify": "^9.1.3",
        "socket.io-client": "^4.7.1",
        "yup": "^1.2.0"
    },
    "devDependencies": {
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/node": "^20.4.2",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "eslint": "^8.45.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "prettier": "^3.0.0",
        "stylelint": "^15.10.1",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-order": "^6.0.3",
        "typescript": "^5.1.6",
        "vite": "^4.4.4",
        "vite-plugin-svgr": "^3.2.0"
    }
}
