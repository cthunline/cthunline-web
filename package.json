        "@mantine/notifications": "^7.11.2",
        "@mantine/tiptap": "^7.11.2",
        "@szhsin/react-menu": "^4.1.0",
        "@tiptap/extension-link": "^2.5.0",
        "@tiptap/extension-text-align": "^2.5.0",
        "@tiptap/extension-underline": "^2.5.0",
        "@tiptap/pm": "^2.5.0",
